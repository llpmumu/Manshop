<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.manshop.android">

    <!-- 联网权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <!-- if you want to load images from a file OR from the internet -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.view.activity.WelcomeActivity" ></activity>
        <activity
            android:name=".ui.view.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
        <activity android:name=".ui.view.activity.LoginActivity" />
        <activity android:name=".ui.view.activity.RegisterActivity" />
        <activity android:name=".ui.view.activity.SearchActivity" />
        <activity android:name=".ui.view.activity.PublishActivity" />
        <activity android:name=".ui.view.activity.AddressActivity" />
        <activity android:name=".ui.view.activity.GoodDetailActivity" />
        <activity android:name=".ui.view.activity.ComicActivity" />
        <activity android:name=".ui.view.activity.EditAddressActivity" />
        <activity android:name=".ui.view.activity.DialogueActivity" />
        <activity android:name=".ui.view.activity.NewPublishActivity" />
        <activity android:name=".ui.view.activity.MyNewOrderActivity"></activity>
        <activity android:name=".ui.view.activity.DeliveryActivity"></activity>
        <activity android:name=".ui.view.activity.OrderDetailActivity"></activity>
    </application>

</manifest>